{
  "name": "express-typescript",
  "version": "1.0.0",
  "description": "Express typescript starter project",
  "main": "index.js",
  "scripts": {
    "start": "TS_NODE_BASEURL=./dist node -r tsconfig-paths/register dist/server.js",
    "serve": "cross-env NODE_ENV=development concurrently \"tsc --watch\" \"TS_NODE_BASEURL=./dist nodemon -r tsconfig-paths/register dist/server.js\"",
    "build": "rimraf dist && tsc",
    "prod": "npm run build && npm run start",
    "testwatch": "jest --watch"
  },
  "devDependencies": {
    "@types/body-parser": "1.19.2",
    "@types/cors": "2.8.12",
    "@types/debug": "4.1.7",
    "@types/express": "4.17.13",
    "@types/iconv-lite": "0.0.1",
    "@types/jest": "28.1.6",
    "@types/lodash": "4.14.182",
    "@types/node": "18.6.3",
    "@typescript-eslint/eslint-plugin": "5.31.0",
    "@typescript-eslint/parser": "5.31.0",
    "concurrently": "7.3.0",
    "eslint": "8.21.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.3.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "28.1.3",
    "node-mocks-http": "1.11.0",
    "nodemon": "2.0.19",
    "prettier": "2.7.1",
    "source-map-support": "0.5.21",
    "ts-jest": "28.0.7",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.0.0",
    "typescript": "4.7.4"
  },
  "author": "Vimson Varghese",
  "license": "ISC",
  "dependencies": {
    "body-parser": "1.20.0",
    "cross-env": "7.0.3",
    "dotenv": "16.0.1",
    "express": "4.18.1",
    "helmet": "5.1.1",
    "iconv-lite": "0.6.3",
    "lodash": "4.17.21",
    "mysql2": "2.3.3",
    "rimraf": "3.0.2"
  }
}
